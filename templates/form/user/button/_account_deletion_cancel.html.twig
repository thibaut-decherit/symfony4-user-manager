<form action="{{ path('account_deletion_cancel') }}" method="post" class="mr-2 disable-on-submit">
    <input type="hidden" name="_csrf_token" value="{{ csrf_token('account_deletion_cancel') }}"/>
    <input type="hidden" name="account_deletion_token" value="{{ user.accountDeletionToken }}"/>
    <button class="btn btn-secondary border-0 rounded-0 text-white" type="submit">
        {{ 'global.form.cancel'|trans }}
    </button>
</form>
